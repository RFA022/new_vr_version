<?xml version="1.0" ?>
<dds version="6.0.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.1/rti_dds_profiles.xsd">

    <qos_library name="QOSLibrary">
        <qos_profile name="DefaultProfile" is_default_qos="true">
            <participant_qos>
                <transport_builtin>
                    <mask>UDPv4</mask>
                </transport_builtin>
                <discovery>
                    <initial_peers>
                        <element>127.0.0.1</element>
                        <element>239.255.0.1</element>
                    </initial_peers>
                </discovery>
                <resource_limits>
                    <type_code_max_serialized_length>64000</type_code_max_serialized_length>
                    <type_object_max_serialized_length>64000</type_object_max_serialized_length>
                </resource_limits>
                <discovery_config>
                    <participant_liveliness_assert_period>
                        <sec>3</sec>
                        <nanosec>0</nanosec>
                    </participant_liveliness_assert_period>
                    <participant_liveliness_lease_duration>
                        <sec>10</sec>
                        <nanosec>0</nanosec>
                    </participant_liveliness_lease_duration>
                </discovery_config>
            </participant_qos>
            <datawriter_qos>
                <property>
                    <value>
                        <element>
                            <name>dds.data_writer.history.memory_manager.fast_pool.pool_buffer_max_size</name>
                            <value>32768</value>
                        </element>
                        <element>
                            <name>dds.data_writer.history.memory_manager.java_stream.min_size</name>
                            <value>4096</value>
                        </element>
                        <element>
                            <name>dds.data_writer.history.memory_manager.java_stream.trim_to_size</name>
                            <value>1</value>
                        </element>
                    </value>
                </property>
            </datawriter_qos>
            <datareader_qos>
                <history>
                    <depth>1</depth>
                </history>
                <reader_resource_limits>
                    <!-- Determines whether the DataReader pre-allocates storage for
                   storing fragmented samples. This setting can be used to limit
                   up-front memory allocation costs in applications that deal with
                   large data -->
                    <dynamically_allocate_fragmented_samples>true</dynamically_allocate_fragmented_samples>
                </reader_resource_limits>
                <property>
                    <value>
                        <!-- The next setting is used to force dynamic memory allocation
                             for samples with a serialized size of larger than 32K. Without

                             setting pool_buffer_max_size, all memory would be obtained
                             from a pre-allocated pool, which would greatly increase the
                             memory usage in an application that sends large data. -->
                        <element>
                            <name>dds.data_reader.history.memory_manager.fast_pool.pool_buffer_max_size</name>
                            <value>32768</value>
                        </element>
                        <element>
                            <name>
                                dds.data_reader.history.memory_manager.java_stream.min_size
                            </name>
                            <value>4096</value>
                        </element>
                        <element>
                            <name>
                                dds.data_reader.history.memory_manager.java_stream.trim_to_size
                            </name>
                            <value>1</value>
                        </element>
                    </value>
                </property>
            </datareader_qos>
        </qos_profile>
    </qos_library>

    <types>
        <include file="DDSCommonTypes.xml"/>
    </types>


    <domain_library name="RFSM_Domain_Library">
        <domain domain_id="3" name="RFSM_Domain">
            <!--MAK_Planner_Gateway-->
            <register_type name="gw_dds_types::LosQueryResponse" type_ref="gw_dds_types::LosQueryResponse"/>
            <register_type name="gw_dds_types::LosQueryRequest" type_ref="gw_dds_types::LosQueryRequest"/>
            <register_type name="gw_dds_types::GeoQueryResponse" type_ref="gw_dds_types::GeoQueryResponse"/>
            <register_type name="gw_dds_types::GeoQuery" type_ref="gw_dds_types::GeoQuery"/>
            <register_type name="gw_dds_types::InitialEntitySnapshotType"
                           type_ref="gw_dds_types::InitialEntitySnapshotType"/>
            <register_type name="gw_dds_types::EntityReportType" type_ref="gw_dds_types::EntityReportType"/>
            <register_type name="gw_dds_types::TickCounter" type_ref="gw_dds_types::TickCounter"/>
            <register_type name="gw_dds_types::EntitySetLocationType" type_ref="gw_dds_types::EntitySetLocationType"/>
            <register_type name="gw_dds_types::AttackEntityCommand" type_ref="gw_dds_types::AttackEntityCommand"/>
            <register_type name="gw_dds_types::StopTasksCommand" type_ref="gw_dds_types::StopTasksCommand"/>
            <register_type name="gw_dds_types::CreateEntityType" type_ref="gw_dds_types::CreateEntityType"/>
            <register_type name="gw_dds_types::AttackReportType" type_ref="gw_dds_types::AttackReportType"/>
            <register_type name="gw_dds_types::ScenarioStatusType" type_ref="gw_dds_types::ScenarioStatusType"/>
            <register_type name="gw_dds_types::TaskStatus" type_ref="gw_dds_types::TaskStatus"/>
            <register_type name="gw_dds_types::EntityMoveCommandType" type_ref="gw_dds_types::EntityMoveCommandType"/>
            <register_type name="gw_dds_types::SpawnSquadCommandType" type_ref="gw_dds_types::SpawnSquadCommandType"/>
            <register_type name="gw_dds_types::PostureType" type_ref="gw_dds_types::PostureType"/>
            <register_type name="gw_dds_types::AimWeaponCommand" type_ref="gw_dds_types::AimWeaponCommand"/>
            <register_type name="gw_dds_types::GetAreasListRequest" type_ref="gw_dds_types::GetAreasListRequest"/>
            <register_type name="gw_dds_types::GetAreasListResponse" type_ref="gw_dds_types::GetAreasListResponse"/>
            <register_type name="gw_dds_types::SetEntityHeadingType" type_ref="gw_dds_types::SetEntityHeadingType"/>
            <register_type name="gw_dds_types::LosToPolygonResponse" type_ref="gw_dds_types::LosToPolygonResponse"/>
			<register_type name="gw_dds_types::LosToPolygonRequest" type_ref="gw_dds_types::LosToPolygonRequest"/>
			<register_type name="gw_dds_types::CreateTacticalGraphicCommandType" type_ref="gw_dds_types::CreateTacticalGraphicCommandType"/>
            <register_type name="gw_dds_types::mobility::NavigationPathPlanningRequest" type_ref="gw_dds_types::mobility::NavigationPathPlanningRequest"/>
			<register_type name="gw_dds_types::mobility::NavigationPathPlanningResponse" type_ref="gw_dds_types::mobility::NavigationPathPlanningResponse"/>


            <topic name="LosToPolygonResponse" register_type_ref="gw_dds_types::LosToPolygonResponse"/>
			<topic name="LosToPolygonRequest" register_type_ref="gw_dds_types::LosToPolygonRequest"/>
            <topic name="LosQueryRequest" register_type_ref="gw_dds_types::LosQueryRequest"/>
            <topic name="LosQueryResponse" register_type_ref="gw_dds_types::LosQueryResponse"/>
            <topic name="GeoQueryRequest" register_type_ref="gw_dds_types::GeoQuery"/>
            <topic name="GeoQueryResponse" register_type_ref="gw_dds_types::GeoQueryResponse"/>
            <topic name="InitialEntitySnapshot" register_type_ref="gw_dds_types::InitialEntitySnapshotType"/>
            <topic name="EntityReport" register_type_ref="gw_dds_types::EntityReportType"/>
            <topic name="TickCounterReport" register_type_ref="gw_dds_types::TickCounter"/>
            <topic name="SetEntityHeadingCommand" register_type_ref="gw_dds_types::SetEntityHeadingType"/>
            <topic name="AttackEntityCommand" register_type_ref="gw_dds_types::AttackEntityCommand"/>
            <topic name="StopTasksCommand" register_type_ref="gw_dds_types::StopTasksCommand"/>
            <topic name="CreateEntity" register_type_ref="gw_dds_types::CreateEntityType"/>
            <topic name="AttackReport" register_type_ref="gw_dds_types::AttackReportType"/>
            <topic name="ScenarioStatus" register_type_ref="gw_dds_types::ScenarioStatusType"/>
            <topic name="TaskStatus" register_type_ref="gw_dds_types::TaskStatus"/>
            <topic name="EntityMoveCommand" register_type_ref="gw_dds_types::EntityMoveCommandType"/>
            <topic name="SpawnSquadCommand" register_type_ref="gw_dds_types::SpawnSquadCommandType"/>
            <topic name="EntityPosture" register_type_ref="gw_dds_types::PostureType"/>
            <topic name="AimWeaponCommand" register_type_ref="gw_dds_types::AimWeaponCommand"/>
            <topic name="GetAreasListRequest" register_type_ref="gw_dds_types::GetAreasListRequest"/>
            <topic name="GetAreasListResponse" register_type_ref="gw_dds_types::GetAreasListResponse"/>
            <topic name="CreateTacticalGraphicCommand" register_type_ref="gw_dds_types::CreateTacticalGraphicCommandType"/>
            <topic name="EntityNavigationPathPlanningRequest" register_type_ref="gw_dds_types::mobility::NavigationPathPlanningRequest"/>
			<topic name="EntityNavigationPathPlanningResponse" register_type_ref="gw_dds_types::mobility::NavigationPathPlanningResponse"/>

        </domain>
    </domain_library>

    <domain_participant_library name="RFSM_Participant_Library">
        <domain_participant name="RFSM_Participant" domain_ref="RFSM_Domain_Library::RFSM_Domain">
            <participant_qos name="RFSM_Participant_Qos" base_name="QOSLibrary::DefaultProfile"/>
            <subscriber name="Subscriber">
                <subscriber_qos/>
                <data_reader name="NavigationPathPlanningResponse_DR" topic_ref="EntityNavigationPathPlanningResponse">
					<datareader_qos base_name="QOSLibrary::DefaultProfile">
						<reliability>
							<kind>RELIABLE_RELIABILITY_QOS</kind>
						</reliability>
					</datareader_qos>
				</data_reader>
                <data_reader name="LosToPolygonResponse_DR" topic_ref="LosToPolygonResponse"/>
                <data_reader name="GetAreasListResponse_DR" topic_ref="GetAreasListResponse"/>
                <data_reader name="LosQueryResponse_DR" topic_ref="LosQueryResponse"/>
                <data_reader name="GeoQueryResponse_DR" topic_ref="GeoQueryResponse"/>
                <data_reader name="EntityReport_DR" topic_ref="EntityReport"/>
                <data_reader name="TickCounterReport_DR" topic_ref="TickCounterReport"/>
                <data_reader name="AttackReport_DR" topic_ref="AttackReport"/>
                <data_reader name="ScenarioStatus_DR" topic_ref="ScenarioStatus">
				    <datareader_qos base_name="QOSLibrary::DefaultProfile">
                        <history>
                            <depth>1</depth>
                        </history>
                        <durability>
                            <kind>TRANSIENT_DURABILITY_QOS</kind>
                        </durability>
						<reliability>
                            <kind>RELIABLE_RELIABILITY_QOS</kind>
                        </reliability>
                    </datareader_qos>
                </data_reader>
                <data_reader name="TaskStatus_DR" topic_ref="TaskStatus">
                    <datareader_qos base_name="QOSLibrary::DefaultProfile">
                        <history>
                            <depth>1</depth>
                        </history>
                        <durability>
                            <kind>TRANSIENT_DURABILITY_QOS</kind>
                        </durability>
                        <reliability>
                            <kind>RELIABLE_RELIABILITY_QOS</kind>
                        </reliability>
                    </datareader_qos>
                </data_reader>
            </subscriber>

            <publisher name="Publisher">
                <publisher_qos/>
                <data_writer name="LosToPolygonRequest_DW" topic_ref="LosToPolygonRequest"/>
                <data_writer name="GetAreasListRequest_DW" topic_ref="GetAreasListRequest"/>
                <data_writer name="LosQueryRequest_DW" topic_ref="LosQueryRequest"/>
                <data_writer name="GeoQueryRequest_DW" topic_ref="GeoQueryRequest"/>
                <data_writer name="InitialEntitySnapshot_DW" topic_ref="InitialEntitySnapshot">
                    <datawriter_qos base_name="QOSLibrary::DefaultProfile">
					    <history>
                            <depth>1</depth>
                        </history>
                        <durability>
                            <kind>TRANSIENT_DURABILITY_QOS</kind>
                        </durability>
						<reliability>
                            <kind>RELIABLE_RELIABILITY_QOS</kind>
                        </reliability>
                    </datawriter_qos>
                </data_writer>
                <data_writer name="SetEntityHeading_DW" topic_ref="SetEntityHeadingCommand"/>
                <data_writer name="AttackEntityCommand_DW" topic_ref="AttackEntityCommand"/>
                <data_writer name="StopTasksCommand_DW" topic_ref="StopTasksCommand"/>
                <data_writer name="CreateEntity_DW" topic_ref="CreateEntity"/>
                <data_writer name="EntityMoveCommand_DW" topic_ref="EntityMoveCommand"/>
                <data_writer name="SpawnSquadCommand_DW" topic_ref="SpawnSquadCommand"/>
                <data_writer name="EntityPosture_DW" topic_ref="EntityPosture"/>
                <data_writer name="AimWeaponCommand_DW" topic_ref="AimWeaponCommand"/>
				<data_writer name="CreateTacticalGraphicCommand_DW" topic_ref="CreateTacticalGraphicCommand"/>
                <data_writer name="NavigationPathPlanningRequest_DW" topic_ref="EntityNavigationPathPlanningRequest"/>
            </publisher>
        </domain_participant>
    </domain_participant_library>
</dds>
